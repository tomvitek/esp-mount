{
  "idf.port": "/dev/ttyUSB0",
  "idf.adapterTargetName": "esp32",
  "files.associations": {
    "**/templates/*.html": "django-html, html",
    "**/templates/*": "django-txt",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements",
  },
  "idf.flashType": "UART",
  "idf.openOcdConfigs": [
    "interface/ftdi/esp32_devkitj_v1.cfg",
    "target/esp32.cfg"
  ],
  "cmake.configureOnOpen": false
}
